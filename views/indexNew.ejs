<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
    <title>Main Page</title>
</head>
<style>
    .container {
        max-width: 243px;
        background: #f8f8f8;
    }

    .container ul {
        list-style: none;
        margin: 0;
        padding: 0;
    }

    .container ul > li {
         border-radius: 4px;
    }


    .bgGreen {
        background: rgba(0, 188, 252, 0.4);
    }
</style>

<body>
    <h1>Search your city:</h1>
    <div>
      <input id="inputText" type="text" size="30" value="">
      <input id="inputHide" type="hidden" value="">
    </div>
    <div class="container">
        <ul id="resultList">
        </ul>
    </div>
</body>
</html>

<script src="/newAutocomplete.js"></script>

<script type="text/javascript">
        $(function () {
            var autocompete = new Autocomplete({
              input: $('#inputText'),
              result: $('#resultList'),
              // url: "/api/Belarus/"
              url: "/api/Russia/"
            });

            autocompete.firstCall();
        });
</script>
